<!--make tabs function properly -->

<script type="text/javascript">

$('.tab').click(function() {
    $(this).siblings().removeClass('currentTab prevTab');
    $(this).addClass('currentTab');
    var prevTab = $(this).prev('.tab');
    $(prevTab).addClass("prevTab");
})

</script>